body {
    margin: 0px;
    padding: 0px;
    background-color: #ededed;
    font: normal 12px "OpenSansRegular";
    color: #666462;
}
/*Dot Out line box Remove in All Browsers CSS*/
body a:active {
    outline: none;
}
body a:focus {
    -moz-outline-style: none;
}
a
body a {
    outline: none;
}
img {
    outline: none;
}
input[type="submit"]::-moz-focus-inner,input[type="button"]::-moz-focus-inner
{
    border: 0px;
}

input[type="reset"]::-moz-focus-inner,input[type="button"]::-moz-focus-inner
{
    border: 0px;
}

/*disabled yellow borders in CHROME*/
input[type="text"],input[type="password"],textarea,select {
    outline: none;
}

/*Body Styles*/
#wrapper {
    width: 100%;
    height: auto;
    position: relative;
}

h1,h2,h3,h4,h5,h6 {
    margin: 0px;
    padding: 0px;
}

p {
    margin: 0px;
    padding: 0px;
}

.clear {
    clear: both;
}

/*Body Styles Left Section*/
#leftsection {
    width: 196px;
    float: left;
    height: auto;
    z-index: 1;
    position: relative;
}

#leftsection_guest {
    width: 196px;
    float: left;
    height: 70%;
    z-index: 2;
    position: relative;
}

.logo {
    height: 127px;
    text-align: center;
}

.logoguest {
    height: 127px;
    padding-bottom: 37px;
    padding-top: 60px;
    text-align: center;
}
