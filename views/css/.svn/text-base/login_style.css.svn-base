.right_container {
    float: left;
    margin: 0 0 0 196px;
    height: auto;
    z-index: 999999;
    z-index: 0;
    position: absolute;
    width: 83%;
}

.right_container_guest {
    float: left;
    margin: 48px 0px 0 196px;
    height: auto;
    z-index: 999999;
    z-index: 0;
    position: absolute;
    width: 85%;
}

*:first-child+html .right_container {
    float: left;
    height: auto;
    z-index: 999999;
    z-index: 1;
    position: relative;
    width: 87%;
    margin: 0 auto 0 auto;
}

#rightsection {
