.awardssec {
    width: 45%;
    float: left;
    height: auto;
}

.awards_active {
    float: left;
    height: 40px; /*instead of auto*/
    width: auto;
    margin: 0px 0px 0px 0px;
    position: relative;
    /*padding-left: 20px;*/
}

.awards_active ul {
    margin: 0;
    padding: 0;
    list-style-type: none;
    text-decoration: none;
}

.awards_active ul li {
    float: left;
    margin: 0;
    padding: 0;
    color: #706e6c;
    text-align: center;
    position: relative;
}

.awards_active a {
    float: left;
    /*margin-right: 8px;*/
    /*margin-left: 5px;*/
    line-height: 33px;
    padding: 0px 9px 0px 9px;
    text-decoration: none;
    color: #706e6c;
    text-align: left;
    text-transform: uppercase;
    font: normal 18px "OswaldLight";
}

.awards_active ul li ul.subLikns {
    display: none;
    position: absolute;
    left: 4px;
    top: 35px;
    z-index: 999;
    background: #02A0BF;
    width: 120px;
}

.awards_active ul li ul.subLikns li {
    display: block;
    background: none;
    float: none !important;
    padding: 0 !important;
    border-top: #404040 solid 1px;
}

.awards_active ul li ul.subLikns li:first-child {
    border: none;
}

.awards_active ul li:hover ul.subLikns {
    display: block;
}

.awards_active ul li ul.subLikns li a {
    float: none;
    display: block;
    text-align: left;
    color: #FFF;
    font: bold 12px "OpenSansRegular";
    background: none !important;
    padding: 5px 0 5px 10px !important;
    margin: 0 !important;
}

.awards_active ul li:hover a span {
    background: url("../images/menu-tab-r.png") 100% 0px no-repeat;
}

.awards_active ul li:hover a {
    background: url("../images/menu-tab-l.png") 0 0 no-repeat;
    color: #FFF;
}

.awards_active ul li:hover a span strong {
    background: url(../images/down_arrow.png) center 28px no-repeat
        transparent;
}

.awards_active a span {
    float: left;
    display: block;
    height: 33px;
    padding: 0 35px 0 21px !important;
    background: url("../images/menu-tab-r.png") 100% -100px no-repeat;
}

.awards_active a:hover,.awards_active a:hover span {
    text-decoration: none;
    color: #fff;
    text-align: left;
}

.awards_active .active a {
    background: url("../images/menu-tab-l.png") 0 0 no-repeat;
    color: #fff;
}

.awards_active .active a span {
    background: url("../images/menu-tab-r.png") 100% 0 no-repeat;
}

.awards_active .active a:hover,.awards_active .active a:hover span {
    text-decoration: none;
    cursor: pointer;
}

.awards_active a:hover {
    text-decoration: none;
    background: url("../images/menu-tab-l.png") 0 0 no-repeat;
    cursor: pointer;
}

.awards_active a:hover span {
    background: url("../images/menu-tab-r.png") 100% 0 no-repeat;
}

.awards_active a strong {
    height: 43px !important;
    display: block;
    font-weight: normal !important;
}

.awards_active .active strong {
    background: url(../images/down_arrow.png) center 28px no-repeat
        transparent;
    z-index: 999999 !important;
    position: relative;
    padding-bottom: 10px;
    font-weight: normal !important;
}

.awards_active a:hover strong {
    background: url(../images/down_arrow.png) center 28px no-repeat
        transparent;
    z-index: 999999;
    font-weight: normal !important;
}

