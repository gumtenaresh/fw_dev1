/*select menu*/
p {
font-family: sans-serif;
}
label.custom-select {
position: relative;
display: inline-block;
}
.custom-select select { font-family:"OpenSansRegular" !important; color:#989898;
display: inline-block;
padding: 3px 3px 3px 5px; font-style:italic !important; font-size:12px !important;
font: inherit;
outline:none; /* remove focus ring from Webkit */

background: #f8f8f8;
-webkit-box-shadow: inset 0px 0px 1px 1px rgba(0, 0, 0, 0.2);
box-shadow: inset 1px 1px 1px 1px rgba(0, 0, 0, 0.2 ); border:none !important;
-webkit-appearance:none; /* remove the strong OSX influence from Webkit */
width:110px;
}

/* for Webkit's CSS-only solution */
@media screen and (-webkit-min-device-pixel-ratio:0) { 
.custom-select select {
padding-right:30px;    
}
}

/* Since we removed the default focus styles, we have to add our own */
.custom-select select:focus {

}

/* Select arrow styling */
.custom-select:after {
content: "";
position: absolute;
top: 0;
right: 0;
bottom: 0;
font-size: 60%;
padding: 0 10px;
background: #c3c3c3 url(../images/select-down.gif) no-repeat 2px 0px;
color: white;
pointer-events:none;
}

.no-pointer-events .custom-select:after {
content: none;
}
