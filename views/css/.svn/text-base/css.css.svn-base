/*left menu*/
.glossymenu{margin:7px 0 0 10px;padding: 0;width: 196px; /*width of menu*/  font-family: 'OswaldBook'; font-size:16px !important;}
.glossymenu a.menuitem{border-left:7px solid #8f8f8f;color:#666462; margin-bottom:5px; display:block; text-transform:uppercase; text-decoration:none;padding:6px 10px 5px 6px;
background:-webkit-gradient(linear, left top, left bottom, from(#ececec), to(#d5d5d5)); background: -moz-linear-gradient(top,  #ececec,  #d5d5d5);outline:2px solid #FFF;outline-ofset:2px;}
.glossymenu a.menuitem:visited, .glossymenu .menuitem:active{}
.glossymenu a.menuitem .statusicon{ /*CSS for icon image that gets dynamically added to headersposition: absolute*/;top:5px;right:5px;border:none;}
.glossymenu a.menuitem:hover{border-left:7px solid #02a0bf; color:#02a0bf;width:182px; text-decoration:none; padding:6px 10px 5px 6px;
	-moz-box-shadow:    inset -10px 0 3px -9px #c3c3c3;
   -webkit-box-shadow: inset -10px 0 3px -9px #c3c3c3;/*d6d6d6*/
   box-shadow:         inset -10px 0 3px -9px #c3c3c3;
}
.glossymenu a.menuitem.active {border-left:7px solid #02a0bf; color:#02a0bf;width:182px; text-decoration:none; padding:6px 10px 5px 6px;
	-moz-box-shadow:    inset -10px 0 3px -9px #c3c3c3;
   -webkit-box-shadow: inset -10px 0 3px -9px #c3c3c3;/*d6d6d6*/
   box-shadow:         inset -10px 0 3px -9px #c3c3c3;
}
.glossymenu div.submenu{ /*DIV that contains each sub menu*/background: #ededed;}
.glossymenu div.submenu ul{list-style-type: none;margin: 0;padding: 0; width:205px;}
.glossymenu div.submenu ul li{ display:block; border-bottom:#FFF solid 2px;}
.glossymenu div.submenu ul li a{display: block;color:#666462; text-transform:uppercase; text-decoration:none;padding:0px 6px 0px 15px;font-size:14px;}
.glossymenu div.submenu ul li a:hover{border-left:7px solid #02a0bf; color:#02a0bf;  background:#FFF; padding-left:8px;}
.glossymenu div.submenu ul li a.active{border-left:7px solid #02a0bf; color:#02a0bf;  background:#FFF; padding-left:8px;}

