.skype2 .selectyzeValue {
	width: 45px;
	height: 18px;
	border: #d2d2d2 solid 1px;
	font-size: 11px;
	color: #333333;
	padding: 0px 5px;
	margin-top: 1px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	-khtml-border-radius: 2px;
	border-radius: 2px;
	color: #333333;
	line-height: 18px;
	background: #faf9f9 url(../images/down_arrow_2.png) no-repeat right
		center;
}

.skype2 .UlSelectize {
	top: 20px;
	left: 0px;
	width: 38px;
	border: 1px solid #666666;
	background: #f5f3f4;
}

.skype2 li a {
	font: 11px Arial;
	background-color: transparent;
	color: #2a98c1;
	width: 38px;
	height: 17px;
}

.skype2 li a:hover {
	color: #fff;
	background-color: #2a98c1; /** gradient bgcolors */
	background-image: -webkit-gradient(linear, left top, left bottom, from(#84d8f8),
		to(#0ab2f1) ); /* Saf4+, Chrome */
	background-image: -webkit-linear-gradient(top, #84d8f8, #0ab2f1);
	/* Chrome 10+, Saf5.1+, iOS 5+ */
	background-image: -moz-linear-gradient(top, #84d8f8, #0ab2f1);
	/* FF3.6 */
	background-image: -ms-linear-gradient(top, #84d8f8, #0ab2f1);
	/* IE10 */
	background-image: -o-linear-gradient(top, #84d8f8, #0ab2f1);
	/* Opera 11.10+ */
	background-image: linear-gradient(top, #84d8f8, #0ab2f1)
}

.skype2 .optgroupTitle {
	margin-top: 15px;
	font-size: 16px;
	padding-left: 10px;
	background-color: #0fb1ef;
	color: #fff;
}