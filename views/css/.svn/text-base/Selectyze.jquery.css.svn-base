.skype .selectyzeValue {
	width: 356px;
	height: 39px;
	border: #d2d2d2 solid 1px;
	margin: 20px 0 0 0;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	-khtml-border-radius: 4px;
	border-radius: 4px;
	color: #333333;
	line-height: 38px;
	padding: 0 10px;
	background: #f5f3f4 url(../images/down_arrow.png) no-repeat right center;
}

.skype .UlSelectize {
	top: 68px;
	left: 0px;
	width: 376px;
	background: #f5f3f4;
}

.skype li a {
	height: 40px;
	font: 14px/30px Arial;
	background-color: transparent;
	color: #2a98c1;
