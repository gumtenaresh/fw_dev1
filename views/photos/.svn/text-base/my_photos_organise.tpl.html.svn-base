{literal}
<script type="text/javascript">
    function changeStatus(n){
        document.getElementById("status").value = n;
        if(n=="1")
        {
            document.getElementById("public").className += " active";
            document.getElementById('private').classList.remove('active');            
        }
        else
        {
            document.getElementById('public').classList.remove('active');
            document.getElementById("private").className += " active";
        }
    }
</script>
{/literal}
