{include file="header2.tpl.html"}

<div class="plr_pts">
    <div class="plr_pts1">
        <P class="para">{$fanwires.name}</P>
        <span>Popular Photos</span>
        <div class="plr_links" id="photoslinks">
            {$popularImages.navigation}
            <a href="#"> </a>
            <a href="#"> </a>
        </div>
    </div>
    <div class="clear"></div>

    <div class="plr_photos">
        <a href="{$popularImages.list.0.title_link}"><img src="{$smarty.const.IMAGE_URL}{$popularImages.list.0.photo}" height="79" width="120" alt=""></a>
        <a href="{$popularImages.list.1.title_link}"><img src="{$smarty.const.IMAGE_URL}{$popularImages.list.1.photo}" height="79" width="120" alt=""></a>
        <a href="{$popularImages.list.2.title_link}"><img src="{$smarty.const.IMAGE_URL}{$popularImages.list.2.photo}" height="79" width="120" alt=""></a>
        <a href="{$popularImages.list.3.title_link}"><img src="{$smarty.const.IMAGE_URL}{$popularImages.list.3.photo}" height="79" width="120" alt=""></a>
        <a href="{$popularImages.list.4.title_link}"><img src="{$smarty.const.IMAGE_URL}{$popularImages.list.4.photo}" height="79" width="120" alt=""></a>
        <a href="{$popularImages.list.5.title_link}"><img src="{$smarty.const.IMAGE_URL}{$popularImages.list.5.photo}" height="79" width="120" alt=""></a>

    </div>

    <div class="clear"></div>
    <div class="plr_pts1">
        <P id="fanwireBioName" class="para">{$boigraphy.name}</P>
        <span>Biography</span>
        <div class="plr_links" id="plr_links_bio">
            {$boigraphy.navigation}
            <a href="#" ></a>
            <a href="#"></a>
        </div>
    </div>
    <div class="clear"></div>

    <div class="plr_about" id="plr_about">
        <div class="plr_about_left">

            <img src="{$smarty.const.IMAGE_URL}{$boigraphy.0.image}" {$boigraphy.heightwidth} alt=""></div>
        <div class="plr_about_right"> 

            <p>{$boigraphy.name}</p> 
            <span>{$boigraphy.0.description|truncate:160}</span>
        </div>

        <div class="clear"></div>

        <div class="plr_about_center" >
            {foreach from=$boigraphy[0] key=k item=v}
            {if $k eq 'image'}

            {/if}
            {if $k eq 'description'}{else}
            <p>{$k}:<span>{$v|substr:0:45}</span></p>
            {/if}

            {/foreach}           
        </div>

    </div>
    <div class="plr_twitter">
        <div class="plr_twitter1"> <img src="{$smarty.const.IMAGE_URL}{$twitterPosts.list.0.photo}" height="50" width="50" alt="" >
            <div class="twitter_name">
                <p>{$fanwires.name}</p>
                <span>Twitter</span></div>
            <div class="twitter_icon"> <a href="#" class="small_icons_tw"></a></div>
        </div>
        <div class="clear"></div>
        <div class="twitter_comments">
            <div style="overflow: auto;height: 300px;">
                {section name=twit loop=$twitterPosts.list}
                {assign var=r value=$twitterPosts.list[twit]}
                <div class="twitter_comment">
                    <p>{$r.description|truncate:60}</p>
                    <p>{$r.cdate}</p>
                </div>
                {/section}
            </div>
            <div class="twitter_bottom">
                <p style="float:left"><a href="#">Share</a></p>
                <p style="float:right"><a href="#">View More</a></p>
            </div>

        </div>
    </div>
    <div class="clear"></div>
    <div class="media">
        <p>Related Media</p>
        {section name=s loop=$relatedArticleNames}
        {assign var=k value=$relatedArticleNames[s]}
        <div class="spear2">
            <div class="spear2_left">
                {if $k.photo}
                <img src="{$smarty.const.IMAGE_URL}{$k.photo}" width="80" height="80*({$k.height}/{$k.width})" />
                {else}
                <img src="{$smarty.const.SITE_URL}views/images/logodef.png" height="53" width="80" alt="" />
                {/if}

            </div>
            <div class="spear2_right">
                <p>name of fanwire</p>
                <span>{$k.title|truncate:40} </span> <span></span> </div>
            <div class="clear"></div>
        </div>

        {/section}


        <div class="see"> <a href="#">See all</a> </div>
    </div>
</div>
</div>
<div class="main_section">
    <div class="main_content">
        <div class="fan_links"><a href="#">Top FanWires</a> &gt; <a href="{$smarty.const.SITE_URL}{$fanwires.url}">{$fanwires.name}</a></div>
        <h1 id="articleTitle">{$articleDetail.0.title}</h1>
        <div class="main_content_inner">
            <div class="mail_details">
                <p>via <span id="artFrom"><a href="{if $articleDetail.0.source eq 1}http://www.facebook.com/{$fanwires.facebook}{else}{$articleDetail.0.article_link}{/if}" target="_blank" style="text-decoration: none;"> {if $articleDetail.0.article_from}{$articleDetail.0.article_from}{else}fanwire{/if} </a></span></p>
                <p>By: <span id="artBy">{if $articleDetail.0.source neq 1}{if $articleDetail.0.article_source eq 0}{$articleDetail.0.article_source|replace:"By":""}{else if $articleDetail.0.article_source eq 1}twitter{else if $articleDetail.0.article_source eq 2}facebook{else}fanwire{/if}{/if}</span></p>
                <p id="artDate" >{assign var=date value=$articleDetail.0.cdate|replace:'ET':''}{$date|date_format:"%a, %d %b %Y %H:%M:%S %z"} </p>
            </div>
            <div class="content_header">
                <div class="content_header_left">


                    <div class="like"><a href="#"><img src="{$smarty.const.SITE_URL}views/images/like.png" height="14" width="15" alt="" > Like </a></div>
                    <!-- AddThis Button BEGIN -->
                    <div class="addthis_toolbox addthis_default_style ">
                        <a class="addthis_button_preferred_1"></a>
                        <a class="addthis_button_preferred_2"></a>
                        <a class="addthis_button_preferred_3"></a>
                        <a class="addthis_button_preferred_4"></a>
                        <a class="addthis_button_compact"></a>

                    </div>

                    <!-- AddThis Button END -->
                    <!--     <div class="like_icons"><a href="#" class="like_gm"></a> <a href="#" class="like_fb"></a> <a href="#" class="like_tw"></a></div>-->
                </div>
                <!--                <div class="select_box">
                                    <form>
                                        <select name="style2" class="selectyze3">
                                        </select>
                                    </form>
                                </div>-->

                <div class="content_header_right">
                    <div class="content_header_right_left_arrow">
                        <a href="javascript:void(0);" onclick="nextArticle('{$artNavigation[0]}','{$articleDetail.0.user_id}');"><img src="{$smarty.const.SITE_URL}views/images/left-arrow.png" height="7" width="10" alt="" >Previous</a>
                    </div>
                    <div class="content_header_right_right_arrow">
                        <a href="javascript:void(0);" onclick="nextArticle('{$artNavigation[1]}','{$articleDetail.0.user_id}')"><img src="{$smarty.const.SITE_URL}views/images/right_arrow.png" height="7" width="10" alt="" >Next</a>
                    </div>
                </div>
            </div>
            <div class="clear"></div>
            <div id="artDes">
                {section name=arDet loop=$articleDetail} {assign var=p value=$articleDetail[arDet]}
                {if $p.photo} 
                <img src="{$smarty.const.IMAGE_URL}{$p.photo}" style="padding-left: 20px;" {$p.heigtwidth} alt="" /> <!--width="336" height="253"-->
                {else}
                <img src="{$smarty.const.IMAGE_URL}{$fanwires.photo}"  style="padding-left: 20px;" height="288" width="373" alt="" /> <!--width="336" height="253"-->
                {/if} 
                {$p.description|replace:'href="../../../':"href=\"{$articleDetail.0.article_sub_link}/"}
                {/section}

            </div>



            <!--            <div class="pages">
                            <div class="links">
                                <p class="pagenation">Page:<a href="#">1</a>l<a href="#">2</a>l<a href="#">3</a> 
                            </div>
                        </div>-->
        </div>
    </div>
    <div class="clear"></div>
    <div class="other_news">
        <p>Other News About <span>{$fanwires.name}</span></p>
        <div class="news_elements" id="mainBlock">
            {section name=fanwire loop=$otherNewsAboutFanwire} 
            {assign var=fan value=$otherNewsAboutFanwire[fanwire]}

            <div class="element item block">
                <div class="element_content">
                    <div class="element_content1">
                        <p>{$fan.name}</p>

                        <a href="#" class="{if $fan.source eq 1}small_icons_fb{else if $fan.source eq 2}small_icons_tw{else if $fan.source eq 3}small_icons_riview{/if}{if $fan.type eq 3}small_icons_vid{else if $fan.type eq 4}small_icons_cam{/if}"></a>
                        <div class="clear"></div>
                        <span id="spancontetnt">
                            {if $fan.title_link neq '' && $fan.source neq 1 && $fan.source neq 2} 
                            <a href='{$fan.title_link}' style="text-transform: none;color: #333333;text-decoration: none;font-size: 14px;font-weight: bold;">{$fan.title}</a>
                            {else if $fan.source neq 1 && $fan.source neq 2}
                            {$fan.title} 

                            {/if}

                        </span> 

                    </div>
                    <p>{$fan.time}</p>
                    <div class="{if $fan.title_link neq '' && $fan.source neq 1 && $fan.source neq 2}video{else if $fan.photo eq ''}image{else}image{/if}"> 
                        {if $fan.source eq 2}
                        <img src="{$smarty.const.IMAGE_URL}{$fan.photo}"  width="75" height="75"/>
                        {else if $fan.photo eq ''}
                        <img src="{$smarty.const.IMAGE_URL}{$fan.fanwire_profile_img}" width="75" height="75"/>
                        {else}

                        <a {if $fan.type eq 0}href="{$fan.link}"{else}href="{$fan.title_link}"{/if}>
                            <img src="{$smarty.const.IMAGE_URL}{$fan.photo}"  width="202" height="{202*$fan.height/$fan.width}"  alt="" >
                        </a>
                        {/if}
                        <p>{if $fan.source eq 1}Facebook{else if $fan.source eq 2}Twitter{else if $fan.source eq 3}Article{/if}

                            {if $fan.type eq 4}Instagram{else if $fan.type eq 3}Youtube{else if $fa.type eq 1}Photos{/if}

                        </p>

                    </div>
                    <p>   {if $fan.title_link neq '' && $fan.source neq 1 && $fan.source neq 2}
                        {$fan.description|truncate:300}
                        {else}
                        {$fan.description}
                        {/if}
                    </p>
                </div>
                <div class="clear"></div>
                <div class="twitter_bottom1">
                    <p style="float:left"><a href="#">Share</a></p>
                    <p style="float:right"><a href="#">View More</a></p>
                </div>
            </div>



            {/section}



        </div>
    </div>
</div>
</div>
{include file="footer.tpl.html"}
