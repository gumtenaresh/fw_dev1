    }
    
    
    function CalAge(mm,dd,yy) {
        var now = new Date();
        bDay = dd + "/" + mm + "/" + yy;
        bD = bDay.split('/');
        if (bD.length == 3) {
            born = new Date(bD[2], bD[1] * 1 - 1, bD[0]);
            years = Math.floor((now.getTime() - born.getTime()) / (365.25 * 24 * 60 * 60 * 1000));
            if (years < 13 ) {
                return 1;

            }else if(years > 100 || years == 100){
                return 2;
            }
            return 0;
        }
    }
  
