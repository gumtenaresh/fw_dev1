            function addFan(id){
                //	alert(id);
                var dataStringa ='id='+id;
    
                $.ajax({
                    type: "POST",
                    url:"<?php echo @SITE_URL;?>
addFanwire",
                    // url:newURL+"fanwireGuest/addFanwire",
                    data: dataStringa,
                    success: function(response) {
                        if(response==1){
                            $("#n"+id).removeClass('fan');
                            $("#n"+id).addClass('unfan');
                            $("#n"+id).val('- unfan');
                            $("#n"+id).removeAttr( 'onClick');
                            $("#n"+id).attr( 'onClick', 'unFan("'+id+'")');
                        }else{
                            $('#message').html(response)
                            .hide()
                            .fadeIn(1500)
                            .fadeOut(1500)
                        }
                    }
                });
                return false;
            }
            function unFan(id){
	
                var dataStringb ='id='+id;
    
