    function expandtext(textArea){
        while (textArea.rows > 1 && textArea.scrollHeight < textArea.offsetHeight){
            textArea.rows--;
        }
        while (textArea.scrollHeight > textArea.offsetHeight)
        {
            textArea.rows++;
        }
        textArea.rows++
    }
