    function submitForm(fanwireid,url)
    {
        var msg = document.getElementById("msg"+fanwireid).value;
        if(msg!=""){
        var dataString = 'id='+fanwireid+'&comment='+msg+'&type=0';
        $.ajax({
            type: "POST",
            url: url,
            data: dataString,
            success: function(result) {
             //alert($.trim(result))
